{
   "version": 2,
   "builds": [
    {
        "src": "api.js", 
        "use": "@now/node"
    }
   ],
   "routes": [
    {
        "src": "/(.*)",
        "dest": "api.js"
    }
   ],
   "rewrites": [
        {
            "source": "/api/(.*)",
        "destination": "api.js"}
   ]
}